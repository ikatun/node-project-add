{
  "name": "node-add",
  "description": "Add eslint, prettier, husky or typescript support to your existing NodeJS or React project.",
  "version": "0.0.1",
  "author": "Ivo Katunaric @ikatun",
  "bin": {
    "node-add": "./bin/run"
  },
  "bugs": "https://github.com/ikatun/node-add/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.19",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "globby": "^10.0.2",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ikatun/node-add",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "node-add",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ikatun/node-add",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
